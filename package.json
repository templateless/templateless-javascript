{
  "name": "templateless-js",
  "version": "0.1.0-alpha.6",
  "description": "Ship faster by sending elegant emails using just code",
  "author": "",
  "keywords": [
    "templateless",
    "email",
    "templates"
  ],
  "license": "MIT",
  "homepage": "https://templateless.com/",
  "bugs": {
    "url": "https://github.com/templateless/templateless-javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/templateless/templateless-javascript.git"
  },
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint '**/*.{js,ts,json}' --fix",
    "format": "prettier --write '**/*.{js,ts,json}'",
    "test": "jest --passWithNoTests",
    "build": "rollup -c --bundleConfigAsCjs",
    "all": "npm run format && npm run lint && npm test && npm run build"
  },
  "browserslist": [
    "last 2 version",
    "> 0.5%",
    "maintained node versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "axios": "1.7.9",
    "base64-js": "1.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "rollup": "4.29.1",
    "ts-jest": "29.2.5",
    "tslib": "2.8.1",
    "typescript": "5.7.2"
  }
}
